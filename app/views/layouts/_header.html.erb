<% if user_signed_in? %>
  <%= image_tag current_user.image.thumb.url if current_user.image? %>
  <p>Login User:<%= current_user.email %>></p>
  <%= link_to 'Log Out', destroy_user_session_path %>
  <%= link_to 'Edit Your Profile', edit_user_registration_path %>
  <%= link_to 'Add Instatgram ', new_admin_instagram_account_path %>
  <%= link_to 'Your Instagram Feed List', admin_instagram_accounts_path %>
<% else %>
  <%= link_to 'Log in', user_session_path %>
  <%= link_to 'Sign in', new_user_registration_path %>
<% end %>
